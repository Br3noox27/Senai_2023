   "source": [
    "df = {\n",
    "    'Código': ['45560','45561', '45562'] ,\n",
    "    'nome':['Empresa A','Empresa B','Empresa C'],\n",
    "    'Produto':['fresadora', 'Gabinete', 'Gabinete'],\n",
    "    'cidade':['Cajamar','Polvilho','Santana de Parnaíba'],\n",
    "    'estado':['Sp','Sp','Sp'],\n",
    "    'Data de Entregra':['24/02', '23/02', '24/02']\n",
    "}\n",
    "df = pd.DataFrame(df)"
