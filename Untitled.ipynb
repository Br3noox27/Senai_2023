   "cell_type": "code",
   "execution_count": 7,
   "id": "82640b2a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Coordenadas da sua empresa\n",
    "nossa_empresa = [-23.4073 , -46.8770]\n",
    "\n",
    "outras_empresas = {\n",
    "    'Empresa A': {\n",
    "        'coords': [-23.3565, -46.8769],\n",
    "        'produtos': ['fresadora'],\n",
    "        'datas': ['24/02'],\n",
    "        'Código':['45560']\n",
    "    },\n",
    "    'Empresa B': {\n",
    "        'coords': [-23.4154188, -46.8461188],\n",
    "        'produtos': ['Gabinete'],\n",
    "        'datas': ['23/02'],\n",
    "        'Código':['45561']\n",
    "    },\n",
    "    'Empresa C': {\n",
    "        'coords': [-23.4539331, -46.8955042],\n",
    "        'produtos': ['Gabinete'],\n",
    "        'datas': ['24/02'],\n",
    "        'Código':['45562']\n",
    "    },\n",
    "}\n",
    "\n",
    "# Criar o mapa com o tipo de mapa Stamen Terrain\n",
    "m2 = folium.Map(location=nossa_empresa, zoom_start=15, tiles='Stamen Terrain')\n",
    "\n",
    "# Adicionar marcador da sua empresa em cada mapa\n",
    "folium.Marker(location=nossa_empresa,popup='Nossa Empresa').add_to(m2)\n",
    "\n",
    "# Adicionar marcadores das outras empresas com informações sobre produtos e datas\n",
    "for nome, info in outras_empresas.items():\n",
    "    coords = info['coords']\n",
    "    produtos = info['produtos']\n",
    "    Códigos = info['Código']\n",
    "    popup_text = '<strong>' + nome + '</strong><br>Produtos: ' + ', '.join(produtos) + '</strong><br>Códigos: ' + ', '.join(Códigos)\n",
    "    folium.Marker(\n",
    "        location=coords,\n",
    "        popup=popup_text,\n",
    "        icon=folium.Icon(color='red')\n",
    "    ).add_to(m2)\n",
    "\n",
    "# Mostrar o mapa\n",
    "\n",
    "m2.save('mapa.html')"
   ]
  },
